<root>
  <control>
    <name>dev_mode</name>
    <params clobber_ok="" map="onoff"></params>
  </control>
  <control>
    <name>fw_up</name>
    <remap>rec_mode</remap>
  </control>
  <control>
    <name>power_state</name>
    <doc>Used to turn the DUT off and on</doc>
    <params cmd="set" interface="servo" drv="lumpy_power" input_type="str"/>
  </control>
  <control>
    <name>init_keyboard</name>
    <doc>initialize keyboard handler</doc>
    <params handler_type="MatrixKeyboard" clobber_ok=""></params>
  </control>
</root>
